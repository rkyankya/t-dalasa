<% presenter = Courses::StudentsPresenter.new(self, @course) %>
<% content_for(:head) do %>
  <title><%= presenter.page_title %></title>
  <%= stylesheet_pack_tag 'CoursesStudentsPack', media: 'all' %>
<% end %>

<% content_for(:tail) do %>
  <%= javascript_pack_tag 'CoursesStudentsPack', nonce: true %>
<% end %>

<% content_for(:inner_wrapper) do %>
  <script id="school-course-students__props" type="application/json"><%= presenter.props_to_json.html_safe %></script>
  <div id="react-root"></div>
<% end %>
