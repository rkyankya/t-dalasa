<% presenter = Courses::ReviewPresenter.new(self, @course) %>

<% content_for(:head) do %>
  <title><%= presenter.page_title %></title>
  <%= stylesheet_pack_tag 'CoursesReviewPack', media: 'all' %>
<% end %>

<% content_for(:tail) do %>
  <%= javascript_pack_tag 'CoursesReviewPack', nonce: true %>
<% end %>

<% content_for(:inner_wrapper) do %>
  <div id="react-root" data-json-props="<%= presenter.props_to_json %>"></div>
<% end %>
